<?xml version="1.0"?>
<block>
  <name>PWM Decoder</name>
  <key>reveng_pwm_decode</key>
  <category>Reverse Engineering</category>
  <import>import reveng</import>
  <make>reveng.pwm_decode($zero_seq, $one_seq)</make>

  <param>
	  <name>Zero Sequence</name>
	  <key>zero_seq</key>
	  <value>(0, 0, 1)</value>
  </param>
  <param>
	  <name>One Sequence</name>
	  <key>one_seq</key>
	  <value>(0, 1, 1)</value>
  </param>

  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

  <source>
    <name>out</name>
    <type>message</type>
  </source>

</block>
